class Solution {
public:
    bool isPalindrome(int x) {
        if(x<0) return false;
        int pali = x ;
        long long rev = 0;
        while (x!=0){
            int ld = x%10;
            x = x/10;
            rev = rev*10 + ld;
        }
        if(pali == rev) return true;
        else return false;
        
    }
};
